{
  "name": "@winner-fed/create-project",
  "version": "1.3.3",
  "description": "An easy way to start a project",
  "type": "module",
  "bin": {
    "create-project": "outfile.cjs"
  },
  "files": [
    "outfile.cjs",
    "template"
  ],
  "engines": {
    "node": "^14.13.1 || >=16.0.0"
  },
  "scripts": {
    "prepare": "husky install",
    "format": "prettier --write .",
    "build": "esbuild --bundle index.js --format=cjs --platform=node --outfile=outfile.cjs",
    "snapshot": "zx ./scripts/snapshot.mjs",
    "pretest": "run-s build snapshot",
    "test": "zx ./scripts/test.mjs",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "prepublishOnly": "zx ./scripts/prepublish.mjs",
    "release": "np"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cloud-templates/create-project.git"
  },
  "keywords": [
    "vue",
    "vue3",
    "create-project"
  ],
  "author": "winner fed",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cloud-templates/create-project/issues"
  },
  "homepage": "https://github.com/cloud-templates/create-project#readme",
  "devDependencies": {
    "ejs": "^3.1.6",
    "esbuild": "^0.13.15",
    "gradient-string": "^2.0.0",
    "husky": "^7.0.4",
    "kolorist": "^1.5.1",
    "lint-staged": "^12.1.4",
    "minimist": "^1.2.5",
    "np": "^7.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "prompts": "^2.4.2",
    "yaml-front-matter": "^4.1.1",
    "zx": "^4.2.0"
  },
  "lint-staged": {
    "*.{js,ts,vue,json}": [
      "prettier --write"
    ]
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
